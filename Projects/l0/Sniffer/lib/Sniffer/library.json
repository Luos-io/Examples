{
    "name": "Sniffer",
    "keywords": "robus,network,microservice,luos,operating system,os,embedded,communication,container,ST",
    "description": "container that receives and transmits all the messages to the pc",
    "version": "0.1.0",
    "authors": {
        "name": "Luos",
        "url": "https://luos.io"
    },
    "licence": "MIT",
    "build": {
